{
  "name": "octon",
  "description": "Notify you when a new release has been made on repositories you starred on Github.",
  "scripts": {
    "start": "node server-build/app.js",
    "start:dev": "babel-watch server/app.js",
    "build": "npm run build:server && npm run build:client",
    "build:server": "node_modules/.bin/babel server --out-dir server-build",
    "build:client": "NODE_ENV=production webpack -p --config config/webpack.production.config.js",
    "test": "npm run lint && npm run test:server",
    "test-travis": "npm run lint && npm run test:server -- --coverage",
    "test:server": "jest server",
    "lint": "eslint server client",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/.*.(test|spec)).(js|jsx)$",
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "engines": {
    "node": "6.1.0",
    "npm": "3.10.5"
  },
